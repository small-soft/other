#! /bin/sh
AN_ROOT='/cygdrive/d/android-ndk-r4' && AN_PLAT="$AN_ROOT/build/platforms/android-3" && PATH="$AN_ROOT/build/prebuilt/windows/arm-eabi-4.4.0/bin:$PATH" ./configure --host=arm-linux CC=arm-eabi-gcc CPPFLAGS="-I$AN_PLAT/arch-arm/usr/include" CFLAGS="-fpic -mthumb-interwork -ffunction-sections -funwind-tables -fstack-protector -fno-short-enums -D__ARM_ARCH_5__ -D__ARM_ARCH_5T__ -D__ARM_ARCH_5E__ -D__ARM_ARCH_5TE__ -Wno-psabi -mtune=xscale -msoft-float -mthumb -Os -fomit-frame-pointer -fno-strict-aliasing -finline-limit=64 -DANDROID -Wa,--noexecstack -O0 -nostdlib -Wl,-shared,-Bsymbolic -Wl,--no-undefined -Wl,-z,noexecstack -Wl,-rpath-link=$AN_PLAT/arch-arm/usr/lib" LIBS="$AN_ROOT/build/prebuilt/windows/arm-eabi-4.4.0/lib/gcc/arm-eabi/4.4.0/libgcc.a $AN_PLAT/arch-arm/usr/lib/libc.so $AN_PLAT/arch-arm/usr/lib/libstdc++.so $AN_PLAT/arch-arm/usr/lib/libm.so" #$AN_PLAT/arch-arm/usr/lib/liblog.so"
