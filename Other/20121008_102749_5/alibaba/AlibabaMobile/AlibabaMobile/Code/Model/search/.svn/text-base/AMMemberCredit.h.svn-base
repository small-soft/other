#import "AMObjectMappingDelegate.h"

/**
 * 用户诚信信息
 * 
 * @author qizhi.tangqz
 */

@interface AMMemberCredit : NSObject <AMObjectMappingDelegate> {

  /**
   * 诚信保障金额
 */
  NSNumber * _creditBalanceYuan;

  /**
   * 身份描述标准图标
 */
  NSString * _trustLogo;

  /**
   * 身份描述小图标
 */
  NSString * _trustLogoSmall;

  /**
   * 诚信指数
 */
  NSNumber * _trustScore;

  /**
   * 是否7天赔付
 */
  NSNumber * _sevenDaysCompensate;

  /**
   * 是否有诚信保障金
 */
  NSNumber * _creditBalance;

  /**
   * 是否有诚信保障
 */
  NSNumber * _garantee;

  /**
   * 是否支持交易
 */
  NSNumber * _tradable;

  /**
   * 是否诚信通会员
 */
  NSNumber * _trust;

  /**
   * 会员类型，参照DataDictConstants中MEMBER_TYPE_
 */
  NSString * _memberType;

  /**
   * 诚信通版本，参照DataDictConstants中TP_VERSION_
 */
  NSString * _tpVersion;
}

@property(nonatomic, retain) NSNumber * creditBalanceYuan;
@property(nonatomic, retain) NSString * trustLogo;
@property(nonatomic, retain) NSNumber * trustScore;
@property(nonatomic, retain) NSNumber * sevenDaysCompensate;
@property(nonatomic, retain) NSNumber * creditBalance;
@property(nonatomic, retain) NSNumber * garantee;
@property(nonatomic, retain) NSNumber * tradable;
@property(nonatomic, retain) NSNumber * trust;
@property(nonatomic, retain) NSString * memberType;
@property(nonatomic, retain) NSString * trustLogoSmall;
@property(nonatomic, retain) NSString * tpVersion;

@end
