#import "AMObjectMappingDelegate.h"

/**
 * 消息中心的查询结果对象类。
 * 
 * @author qizhi.tangqz 2012-2-25 
 */

@interface AMMobileMessageQueryResult : NSObject <AMObjectMappingDelegate> {

  /**
     * 符合条件的总消息条数
  */
  NSNumber * totalCount;

  /**
     * 本次返回的消息条数
  */
  NSNumber * resultCount;

  /**
     * 具体消息内容
  */
  NSArray * messages;
}

@property(nonatomic, retain) NSNumber * totalCount;

@property(nonatomic, retain) NSNumber * resultCount;
@property(nonatomic, retain) NSArray * messages;

@end
